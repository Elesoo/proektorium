{% block content %}
	<div class="section">
		<p>ВАШ ЛИЧНЫЙ КАБИНЕТ</p>
		<div id="leftbar">
			<div class="crd profile-card">
				<p>{{ employee.personal.surname }} {{ employee.personal.name }} {{ employee.personal.patr }}</p>
				<img class="img_profile" id="avatar" width=200 height=200 src="/user/getAvatar">
				<div class="crd-buttons crd-buttons-inline crd-buttons-vertical">
					<button type="button" class="btn btn-profile" onclick="load_input.click()">Изменить фото</button>
					<input id="load_input" class="sr-only" type="file" accept="image/*">
					<button type="button" class="btn btn-profile" data-toggle="modal" data-target="#resetPassword">Изменить пароль</button>
				</div>
			</div>

			<div class="crd statusicons-card">
				<p class="crd-caption crd-caption-left">Мои статусы</p>
				{% if employee.statuses %}
					{% for status in employee.statuses %}
						<img width=60 height=60 src="img/logo.png">
					{% endfor %}
				{% else %}
					<p>У вас пока нет статусов</p>
				{% endif %}
			</div>

			<div class="crd statusicons-card">
				{% if is_granted("ROLE_EMPLOYEE_ACCESS_CLAIM_SENDED") %}
					<button onclick="alert('Ваша заявка на получения доступа к спискам студентов уже отправлена модератору! Пожалуйста, дождитесь на нее ответа');" class="btn btn-wait">Заявка подана</button>
				{% elseif is_granted("ROLE_EMPLOYEE_ACCESS_STUDENTS") %}
					<p class="crd-caption" style="text-align: center;">Управлениe списками студентов</p>
					<div class="group_div">
						<label class="group_label" for="group">Группа</label>
				{# <select class="group_select" id="group">
					<option>ВПР-21</option>
					<option>ВПР-23</option>
				</select> #}
				{% set class = 'group_select js-example' %}
					{% set id = 'group' %}
						{% include 'user/groups.html.twig' with [class, id] %}
					</div>
					<button id="getlist" class="btn btn-wait" onclick="selectGroup();">Скачать список</button>
				{% else %}
					<button onclick="sendAccessClaim();" class="btn btn-create">Запросить доступ к управлению списками студентов </button>
				{% endif %}
			</div> 

			<!-- Добавить ссылку на подразделение через onclick -->
			<div class="crd">
				{% if is_granted("ROLE_DEPARTMENT_ADMINISTRATOR") %}
