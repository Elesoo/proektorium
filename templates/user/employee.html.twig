	<script type="text/javascript">
	function getMyProject(projid, containerid)
	{
		$.ajax({
			url:"/user/employee/projectCard/"+projid,
			type:'POST',
			success: function(data){
				$('#containerProject'+containerid).html(data);
			}
		});
	}
	function getMyDaynProject(projid, containerid)
	{
		$.ajax({
			url:"/user/employee/dayn/projectCard/"+projid,
			type:'POST',
			success: function(data){
				$('#containerProject'+containerid).html(data);
			}
		});
	}
	function getMyEvent(eventId, containerid)
	{
		$.ajax({
			url:"/user/employee/eventCard/"+eventId,
			type:'POST',
			success: function(data){
				$('#containerEvent'+containerid).html(data);
			}
		});
	}
	function getProjectInvite(projid, roleid, containerid)
	{
		$.ajax({
			url:"/user/employee/showInvite/"+projid+"/"+roleid,
			type:'GET',
			success: function(data){
				$('#container'+containerid).html(data);
			}
		});
	}
	function getDayNProjectInvite(projid, containerid)
	{
		$.ajax({
			url:"/user/employee/dayn/showInvite/"+projid,
			type:'GET',
			success: function(data){
				$('#containerdayn'+containerid).html(data);
			}
		});
	}
	function denyDaynRole(projid)
	{
		$.ajax({
			url:"/user/employee/dayn/denyInvite/"+projid,
			type:'POST',
			success: function(data){
				alert("Заявка отклонена!");
				window.location.href = "/user";
			},
			error: function(data){
				alert("Ошибка при отклонении заявки!");
			}
		});
	}
	function allowDaynRole(projid)
	{
		$.ajax({
			url:"/user/employee/dayn/allowInvite/"+projid,
			type:'POST',
			success: function(data){
				alert("Заявка принята! Теперь вы руководите этим проектом!");
				window.location.href = "/user";
			},
			error: function(data){
				alert("Ошибка при принятии заявки!");
			}
		});
	}
	function denyRole(projid, roleid)
	{
		$.ajax({
			url:"/user/employee/project/"+projid+"/denyRole",
			type:'POST',
			data: {role: roleid},
			success: function(data){
				alert("Заявка отклонена!");
				window.location.href = "/user";
			},
			error: function(data){
				alert("Ошибка при отклонении заявки!");
			}
		});
	}
	function allowRole(projid, roleid)
	{
		$.ajax({
			url:"/user/employee/project/"+projid+"/allowRole",
			type:'POST',
			data: {role: roleid},
			success: function(data){
				if (data == "busy")
					alert("Роль уже занята!");
				else
					alert("Заявка принята!");
				window.location.href = "/user";
			},
			error: function(data){
				alert("Ошибка при принятии заявки!");
			}
		});
	}
	function allowRoleByCurator(projid, roleid, studid)
	{
		$.ajax({
			url:"/user/employee/curator/"+projid+"/allowRole",
			type:'POST',
			data: {role: roleid, stud: studid},
			success: function(data){
				if (data == "all good")
				{
					alert("Заявка успешно одобрена!");
					window.location.href = "/user";
				}
			},
			error: function(data){
				if (data == "Didn't allow to hold more than one role in one project")
				{
					alert("Нельзя принимать студента на две роли сразу в один проект!");
					window.location.href = "/user";
				}
				else
					alert("Не удалось принять заявку!");
			}
		});
	}
	function denyRoleByCurator(projid, roleid, studid)
	{
		$.ajax({
			url:"/user/employee/curator/"+projid+"/denyRole",
			type:'POST',
			data: {role: roleid, stud: studid},
			success: function(data){
				if (data == "all good")
				{
					alert("Заявка успешно отклонена!");
					window.location.href = "/user";
				}
			},
			error: function(data){
				alert("Не удалось отклонить заявку!");
			}
		});	
	}
	function getClaimStud(projid, roleid, studid, containerid)
	{
		$.ajax({
			url:"/user/employee/curator/"+projid,
			type:'POST',
			data: {role: roleid, stud: studid},
			success: function(data){
				$('#claimCtr'+containerid).html(data);
			}
		});
	}
	function sendAccessClaim()
	{
		$.ajax({
			url:"/user/employee/createaccessclaim",
			type:'GET',
			success: function(data){
				alert("Ваша заявка успешна подана! Как только модератор ее одобрит, вы получите доступ к просмотру данных о студентах!");
				window.location.href = "/user";
			},
			error: function(data){
				alert("Вы уже подавали заявку! Пожалуйста, дождитесь ответа модератора!");
			},
		});
	}
	function selectGroup()
	{
	// console.log($("#group option:selected").text());
	if ($("#group option:selected").text() == "")
		alert("Выберите группу в выпадающем списке!");
	else
		window.location.href = "/user/employee/getGroup/"+$("#group option:selected").text();
}
</script>

