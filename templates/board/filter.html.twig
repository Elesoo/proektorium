{% block styles_and_other_shit %}
    <style>
		.select2{
			width: 100% !important;
			margin-bottom: 10px;
		}

		.select2-selection__choice{
			margin-bottom: 5px;
			white-space: pre-wrap;
			word-wrap: break-word;
			width: 100%;
		}

		.input-select{
			width: 100%;
			box-sizing: border-box;
			border-radius: 4px;
			border: 1px solid #aaa;
			outline: none;
			padding: 4px 5px 3px;
			cursor: text;
			font-size: 16px;
			margin-bottom: 10px;
			background-color: #fff;
		}

		.select2-selection__choice__remove{
			display: inline !important;
		}

		.select2-container--default .select2-selection--multiple {
			border-radius: 10px;
		}

		.input-select:focus{
			border: 1px solid #555;	
		}

		.claster__select {
			width: 100%;
			height: 29px;
			border: 0.2px solid #000;
			font-weight: 300;
			font-size: 12px;
			-ms-text-align-last: center;
			text-align-last: center;
			outline: none !important;
			line-height: 16px;
			border-radius: 5px;
			background-color: #fff;
			-webkit-appearance: none;
			-moz-appearance: none;
			-ms-appearance: none;
			appearance: none;
			background-blend-mode: multiply;
			background-image: url('/img/ar.png') no-repeat 95% center;
			vertical-align: middle;
		}

		.claster__select option {
			text-align: center;
		}

		.flex__cont {
			display: -webkit-inline-flex;
			display: -moz-inline-flex;
			display: -ms-inline-flex;
			display: -o-inline-flex;
			display: inline-flex;
			-webkit-flex-direction: row;
			-moz-flex-direction: row;
			-ms-flex-direction: row;
			-o-flex-direction: row;
			flex-direction: row;
			width: 100%;
		}

		.dayn__show_btn {
			width: 100%;
			height: 29px;
			background: #FFFFFF;
			border: 1.5px solid #0C7CBB;
			font-size: 13px;
			line-height: 18px;
			text-align: center;
			color: #0C7CBB;
			border-radius: 10px;
		}

		@media(min-width: 1200px){
			.row > div { 
				padding-right: 0px;
			}
			.row > div:last-child{
				padding-right: 13px;
			}
		}
    </style>
{% endblock %}

