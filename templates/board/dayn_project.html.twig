<script>
	function sendStudentClaim(id) {
	$.ajax({
		url: "/project/dayn/sendClaim/"+id,
		method: "POST",
		processData: false,
		contentType: false,

		success: function() {
			alert("Заявка отправлена");
			window.location.href = "/project/dayn/show/"+id;
		},
		error: function() {
			alert("Ошибка");
		}
	});
}
</script>

