<script type="text/javascript">
function getFullRole(roleid, containerid)
{
  $.ajax({
    url:"/project/role/"+roleid+"/"+{{ project.id }},
    type:'GET',
    success: function(data){
      $('#container'+containerid).html(data);
    }
  });
}
function sendClaim(roleidv, studidv)
{
  $.ajax({
    url:"/project/claimStudent/"+{{ project.id }},
    data: {studid: studidv, roleid: roleidv},
    type:'POST',
    success: function(data){
      if (data == "already")
        alert("Вы уже отправляли подобную заявку или вас приглашал модератор!")
      else
        alert("Ваша заявка отправлена! Ожидайте ответа от модератора!");
      location.reload();
    },
    error: function(data){
      alert("Ошибка при подаче заявки!")
    }
  });
}
function sendEmployeeClaim(roleidv, employeeidv)
{
  $.ajax({
    url:"/project/claimEmployee/"+{{ project.id }},
    data: {employeeid: employeeidv, roleid: roleidv},
    type:'POST',
    success: function(data){
      if (data == "already")
        alert("Вы уже отправляли подобную заявку или вас приглашал модератор!")
      else{
        alert("Ваша заявка отправлена! Ожидайте ответа от модератора!");
        location.reload();
      }

    },
    error: function(data){
      alert("Ошибка при подаче заявки!")
    }
  });
}
function getBusyRole(role, project, key)
{
  $.ajax({
    url:"/project/busyRole/"+project+"/"+role,
    type:'GET',
    success: function(data){
      $('#container'+key).html(data);
    },
  });
}
function denyPersonFromRole(project, role, student)
{
  $.ajax({
    url:"/project/roles/"+project+"/"+role+"/"+student+"/denyPerson",
    type:'POST',
    success: function(data){
      alert("Человек успешно снят с данной роли!");
    },
    error: function(data){
      alert("Не удалось снять человека с заявки!");
    }
  });
}
function denyPersonFromMentorCurator(project, role, userid)
{
  $.ajax({
    url:"/project/roles/"+project+"/"+role+"/"+student+"/denyMentorCurator",
    type:'POST',
    success: function(data){
      alert("Человек успешно снят с данной роли!");
    },
    error: function(data){
      alert("Не удалось снять человека с заявки!");
    }
  });
}
function getMentor() {
  $.ajax({
    url:"/project/getMentor/"+{{ project.id }},
    type:'GET',
    success: function(data){
      $("#mentorCard").html(data);
    }
  });
}
function getCurator(){
  $.ajax({
    url:"/project/getCurator/"+{{ project.id }},
    type:'GET',
    success: function(data){
      $("#curatorCard").html(data);
    }
  });
}
</script>
