{% block content %}
  {% if project %}
  <!-- Modal -->
  <div class="modal fade" id="changeContactForm" tabindex="-1" role="dialog" aria-labelledby="modalTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="modalTitle">Контактное лицо</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">

          <div id="creds" class="tab-pane fade in active show">
            <div class="form-group row">
              <label for="livingplace" class="col-sm-3 col-form-label">ФИО</label>
              <div class="col-sm-9">
                <input class="forminput" id="fio" value="$contact_name">
              </div>
            </div>
            <div class="form-group row">
              <label for="livingplace" class="col-sm-3 col-form-label">Телефон</label>
              <div class="col-sm-9">
                <input class="forminput" id="phone" value="$contact_phone">
              </div>
            </div>
            <div class="form-group row">
              <label for="livingplace" class="col-sm-3 col-form-label">E-mail</label>
              <div class="col-sm-9">
                <input class="forminput" id="email" value="$contact_email">
              </div>
            </div>
          </div>


        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-primary" onclick="sendForm()">Сохранить</button>
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Закрыть</button>
        </div>
      </div>
    </div>
  </div>



  <div id="proj_header" style="background-image: url('/project/header/{{ project.docs['headerImage'] }}'); background-repeat: no-repeat; background-size: 100%; background-position: center;">
  </div>
  </div>

  <div class="block" style="background: #fff; border-radius: 20px;padding: 20px;">


    <div style="overflow: auto;" class="section">
      <div class="float__block_text">
        <p class="univ__text">{{ project.info.title }}</p>
      </div>  
      <!-- 		<p class="project-types-container"><span>{{ project.info.state }}</span><span>{{ project.info.specialization|trans }}</span><span>{{ project.info.industry }}</span></p> -->
      <p class="project-types-container project__subtitle">{{ project.info.state }}</p>
      <p class="project-types-container project__subtitle">{{ project.info.specialization|trans }}</p>
      <p class="project-types-container project__subtitle marl">{{ project.info.industry }}</p>
      <h2 style="font-weight: bold;">Описание</h2>
      <p>{{ project.info.description |raw |nl2br}}</p>
      <h2 style="font-weight: bold;">Продуктовый результат</h2>
      <p>{{ project.info.product|raw |nl2br }}</p>
      <h2 style="font-weight: bold;">Срок реализации </h2>
      <p>{{ project.info.date_start|date("d.m.Y") }} - {{ project.info.date_close|date("d.m.Y") }}</p>
      <h2 style="font-weight: bold;">Местоположение</h2>
      <p>{{ project.info.place }}</p>


      <div class="proj__deal">
        <p class="project__text">Заказчик: {{ project.info.creator }}</p>
      </div>
    </div>
    {% if project.state < 3 %}
    <div class="container-fluid p-0">
      <p class="section-caption" style="font-weight: bold;">Команда</p>
      <div class="row">
        {% if project.freeRoles %}
        {% for key,role in project.freeRoles %}
        {% if is_granted("ROLE_STUDENT") and role.role == "curator"%}

        {% elseif is_granted("ROLE_EMPLOYEE") and role.role == "mentor" %}

        {% else %}
        <div id="container{{ key }}" class="col-md-6 col-lg-6 col-xl-4 col-xxl-3 pr-0 mb-4" style="text-align: center;">
          <script type="text/javascript">
          $('#container{{ key }}').on('show', getFullRole("{{ role.role }}", {{ key }} ));
          </script>
          <img style="width: 20%; height: auto;" src="/img/loading.gif">
        </div>
        {% endif %} {# end if is_granted #}
        {% endfor %}
        {% endif %} {# end if free roles #}
        {# Draw mentor and curator cards #}
        {% for card in project.people %}
        {% if card['role'] == 'mentor' and card['person'] != "" %}
        <div class="col-md-6 col-lg-6 col-xl-4 col-xxl-3 pr-0 mb-4">
          <div id="mentorCard" style="text-align: center;">
            <script type="text/javascript">
            $('#mentorCard').on('show', getMentor());
            </script>
            <img style="width: 20%; height: auto;" src="/img/loading.gif">
          </div>
        </div>
        {% endif %}
        {% if card['role'] == 'curator' and card['person'] != "" %}
        <div class="col-md-6 col-lg-6 col-xl-4 col-xxl-3 pr-0 mb-4">
          <div id="curatorCard" style="text-align: center;">
            <script type="text/javascript">
            $('#curatorCard').on('show', getCurator());
            </script>
            <img style="width: 20%; height: auto;" src="/img/loading.gif">
          </div>
        </div>
        {% endif %}
        {# Busy roles #}
        {% if card.person != "" and (card.role != 'mentor' and card.role != 'curator') %}
        {% set key = random(1000) %}
        <script type="text/javascript">
        $('#container{{ key }}').on('show', getBusyRole("{{ card.role }}", {{ project.id }},{{ key }} ));
        </script>

        <div id="container{{key}}" class="col-md-6 col-lg-6 col-xl-4 col-xxl-3 pr-0 mb-4" >
          <div class="teammates-caption" style="text-align: center;">
            <img style="width: 20%; height: auto;" src="/img/loading.gif">
          </div>
        </div>
        {% endif %}
        {% endfor %}
      </div>
    </div> 

